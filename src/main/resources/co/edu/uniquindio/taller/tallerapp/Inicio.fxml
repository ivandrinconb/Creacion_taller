<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="725.0" prefWidth="829.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.taller.tallerapp.controller.InicioController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button fx:id="btnRegistroClientes" mnemonicParsing="false" onAction="#RegistroClientes" prefHeight="51.0" prefWidth="165.0" text="Registro de clientes " />
      <Button fx:id="btnRegistroBicicleta" mnemonicParsing="false" onAction="#RegistroBicicletas" prefHeight="50.0" prefWidth="165.0" text="Registro de bicicletas" />
      <Button fx:id="btnRegistroMecanico" mnemonicParsing="false" onAction="#RegistroMecanicos" prefHeight="51.0" prefWidth="165.0" text="Registro Mecanicos" />
      <Button fx:id="btnRegistroOrden" mnemonicParsing="false" onAction="#RegistroOrden" prefHeight="51.0" prefWidth="165.0" text="Registro de ordenes" />
   </children>
</VBox>
